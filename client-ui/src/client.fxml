<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainClientController">
   <children>
      <VBox alignment="CENTER">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="22.0" prefWidth="55.0" text="Client">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="85.0" prefWidth="600.0" spacing="20.0">
               <children>
                  <VBox alignment="TOP_CENTER" prefHeight="0.0" prefWidth="120.0">
                     <children>
                        <Button fx:id="simDetailsButton" mnemonicParsing="false" onAction="#handleSimDetailsButton" prefHeight="25.0" prefWidth="117.0" text="Simulation Details" textAlignment="CENTER">
                           <VBox.margin>
                              <Insets top="48.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
                  <VBox alignment="TOP_CENTER" layoutX="390.0" layoutY="10.0" prefHeight="0.0" prefWidth="120.0">
                     <children>
                        <Button fx:id="requestButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefWidth="100.0" text="Requests" textAlignment="CENTER">
                           <VBox.margin>
                              <Insets top="48.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
                  <VBox alignment="TOP_CENTER" layoutX="195.0" layoutY="10.0" prefHeight="0.0" prefWidth="120.0">
                     <children>
                        <Button fx:id="executionButton" mnemonicParsing="false" prefWidth="100.0" text="Execution" textAlignment="CENTER">
                           <VBox.margin>
                              <Insets top="48.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
                  <VBox alignment="TOP_CENTER" layoutX="260.0" layoutY="10.0" prefHeight="0.0" prefWidth="120.0">
                     <children>
                        <Button fx:id="resultsButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefWidth="100.0" text="Results" textAlignment="CENTER">
                           <VBox.margin>
                              <Insets top="48.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets top="-20.0" />
               </padding>
            </HBox>
            <Label fx:id="usernameLabel" alignment="TOP_LEFT" contentDisplay="CENTER" prefHeight="17.0" prefWidth="365.0" text="Username:" textAlignment="CENTER" />
         </children>
      </VBox>
      <ScrollPane>
         <content>
            <VBox fx:id="currentSceneHolder" prefHeight="323.0" prefWidth="600.0" />
         </content>
      </ScrollPane>
   </children>
</VBox>
