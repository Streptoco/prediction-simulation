<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controllers.AdminChooseController">
   <children>
      <VBox prefHeight="323.0" prefWidth="600.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="58.0" prefWidth="600.0" spacing="15.0">
               <children>
                  <TextField fx:id="filaPathField" prefHeight="25.0" prefWidth="335.0" />
                  <Button fx:id="loadFileButton" mnemonicParsing="false" onAction="#onFileLoad" prefHeight="25.0" prefWidth="73.0" text="Load File" textAlignment="CENTER" />
                  <Button fx:id="threadCountButton" mnemonicParsing="false" onAction="#submitFile" prefHeight="25.0" prefWidth="147.0" text="Set Thread count" textAlignment="CENTER" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets left="5.0" />
               </padding>
            </HBox>
            <HBox prefHeight="263.0" prefWidth="600.0">
               <children>
                  <TreeView fx:id="simulationsTreeView" prefHeight="263.0" prefWidth="407.0" />
                  <Separator orientation="VERTICAL" prefHeight="263.0" prefWidth="7.0" />
                  <ListView fx:id="threadListView" prefHeight="200.0" prefWidth="200.0" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</VBox>
