<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controllers.UserRequestController">
   <children>
      <VBox fx:id="currentSceneHolder" prefHeight="323.0" prefWidth="600.0">
         <children>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <VBox spacing="18.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="17.0" prefWidth="101.0" text="Simulation Name:">
                           <padding>
                              <Insets top="5.0" />
                           </padding></Label>
                        <Label alignment="CENTER" prefHeight="17.0" prefWidth="101.0" text="Amount of runs:" />
                     </children>
                     <padding>
                        <Insets top="3.0" />
                     </padding>
                  </VBox>
                  <VBox fillWidth="false" spacing="10.0">
                     <children>
                        <ComboBox fx:id="simulationChooseMenu" onAction="#setSimulations" prefHeight="25.0" prefWidth="162.0" promptText="Choose Simulation" />
                        <TextField fx:id="amountRunField" prefHeight="25.0" prefWidth="47.0" />
                     </children>
                     <padding>
                        <Insets top="3.0" />
                     </padding>
                  </VBox>
                  <VBox prefHeight="77.0" prefWidth="237.0">
                     <children>
                        <HBox prefHeight="80.0" prefWidth="237.0">
                           <children>
                              <Label prefHeight="24.0" prefWidth="81.0" text="Termination:" />
                              <ComboBox fx:id="terminationMenu" onAction="#setComboMenuItemTermination" prefHeight="34.0" prefWidth="135.0" promptText="Termination" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="5.0">
                           <children>
                              <Label prefHeight="17.0" prefWidth="30.0" text="Tick:" />
                              <TextField fx:id="tickField" prefHeight="25.0" prefWidth="47.0" />
                              <Label prefHeight="17.0" prefWidth="30.0" text="Time:" />
                              <TextField fx:id="timeField" prefHeight="25.0" prefWidth="47.0" />
                           </children>
                           <padding>
                              <Insets top="5.0" />
                           </padding>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox spacing="15.0">
                     <children>
                        <Button fx:id="submitButton" alignment="CENTER" disable="true" mnemonicParsing="false" onAction="#createNewRequest" prefHeight="25.0" prefWidth="64.0" text="Submit" />
                        <Button fx:id="executeButton" alignment="CENTER" disable="true" mnemonicParsing="false" prefHeight="25.0" prefWidth="64.0" text="Execute" />
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets top="15.0" />
               </padding>
            </HBox>
            <TableView fx:id="requestTable" prefHeight="219.0" prefWidth="589.0">
              <columns>
                <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="statusColumn" prefWidth="75.0" text="Status" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </padding>
      </VBox>
   </children>
</VBox>
